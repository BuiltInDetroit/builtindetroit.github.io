{
  "name": "@builtindetroit/builtindetroit-kaigi",
  "version": "1.0.0",
  "description": "Simple, clean Hugo theme designed for events and workshops.",
  "private": true,
  "license": "MIT",
  "type": "module",
  "repository": {
	  "type": "git",
	  "url": "git+https://github.com/BuiltInDetroit/builtindetroit.github.io"
	},
	"author": "Built In Detroit Project Contributors <hello@mfgopen.org>",
	"bugs": {
	  "url": "https://github.com/BuiltInDetroit/builtindetroit.github.io/issues"
	},
	"homepage": "https://github.com/BuiltInDetroit/builtindetroit.github.io",
  "engines": {
    "node": ">=20",
    "pnpm": ">=8",
    "yarn": "Use pnpm",
    "npm": "Use pnpm"
  },
  "packageManager": "pnpm@8.12.0",
  "scripts": {
    "build:dev": "pnpm tailwindcss -c ./tailwind.config.ts -i ./assets/app.css -o ./assets/main.css --watch",
    "build:prod": "pnpm tailwindcss -c ./tailwind.config.ts -i ./assets/app.css -o ./assets/main.css --minify"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {}
}
